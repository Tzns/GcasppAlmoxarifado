@page
@model Almoxarifado.Pages.Execucao.Entrada.IndexModel
@{
}

<h1>Entradas</h1>
<form method="get" class="d-flex">
    <div class="input-group">
        <input class="form-control-sm" placeholder="Pesquisar" asp-for="Pesquisar">
        <button class="btn btn-outline-secondary btn-light btn-sm" type="submit">
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-search" viewBox="0 0 16 16">
                <path d="M11.742 10.344a6.5 6.5 0 1 0-1.397 1.398h-.001c.03.04.062.078.098.115l3.85 3.85a1 1 0 0 0 1.415-1.414l-3.85-3.85a1.007 1.007 0 0 0-.115-.1zM12 6.5a5.5 5.5 0 1 1-11 0 5.5 5.5 0 0 1 11 0z" />
            </svg>
        </button>
    </div>
</form>
<br />

<p>
    <a class="btn btn-primary" asp-page="./Create"> Novo</a>

    <table class="table">
        <thead>
            <tr>
                <th>
                    Id
                </th>
                <th>
                    Data
                </th>
                <th>
                    Documento
                </th>
                <th>
                    Empenho
                </th>
                <th>
                    CNPJ
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.gEntrada)
            {
                <tr>
                    <td>
                        @item.Id
                    <td>
                        @item.Data
                    </td>
                    <td>
                        @item.Documento
                    </td>
                    <td>
                        @item.Empenho
                    </td>
                    <td>
                        @item.GFornecedor.CNPJ
                    </td>
                </tr>
            }
        </tbody>
    </table>
<p>